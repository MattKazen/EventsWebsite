@model IEnumerable<COP4710_V2.Models.Rso>

@{
	ViewData["Title"] = "Index";
}

<h2>Pending RSO's at @ViewBag.UniName </h2>


<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.RsoId)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Name)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.RsoAdmin.AdminEmail)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.RsoUniversity)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.NumMembers)
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>

		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.RsoId)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Name)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.PendingRso.PendingRsoCreator.UserName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.RsoUniversity.UniName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.NumMembers)
				</td>
				<td>
					<a asp-action="JoinRso" asp-route-id="@item.RsoId">Join Rso</a>

				</td>
			</tr>
		}
	</tbody>
</table>
